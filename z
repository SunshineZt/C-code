

#include<stdio.h>

int main()

{

    int arr[5],i,mini,maxi,temp;

    printf("input
intergets:");

    for(i=0;i<5;i++)

        scanf("%d",&arr[i]);

    for(i=0;i<5;i++)

        printf("%d",arr[i]);

    mini=maxi=0;

    for(i=0;i<5;i++)

    {

        if(arr[i]>arr[maxi])

            maxi=i;

        else if(arr[i]<arr[maxi])

            maxi=i;

        }

        printf("\ninput:");

        for(i=0;i<5;i++)

        {

    temp=arr[mini];

    arr[mini]=arr[maxi];

    arr[maxi]=temp;

    printf("%d",arr[i]);

}

    return 0;

    

}

