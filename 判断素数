

#include<stdio.h>

#include<stdlib.h>

 

int main()

{   

    int i,j,flag;

    int count;

    count=0;

 

    for(i=2;i<1000;i++)

    {

            flag=1;

        for(j=2;j<i;j++)

            if(i%j==0)

            {

                flag=0;

                j=i;

            }

    if(flag==1)

    { 
printf("%4d",i);

        count++;

    }

}

printf("\n%d",count);

    return 0;

    

}  

